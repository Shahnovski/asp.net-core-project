
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <html>
    <body>
        <div>
            <h3>Кафедры</h3>
            @foreach (var b in ViewBag.Departments)
            {
             <div class="well">
                 <h3>
                     <strong>@Html.RouteLink((string)b.Title, new { controller = "Department", action = "viewDepartment", departmentId = b.Id })</strong>
                 </h3>
             </div>
            }
        </div>
    </body>
    </html>

